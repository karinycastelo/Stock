<div class="container">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name), html: { role: 'form'}) do |f| %>
    <%= devise_error_messages! %>
    <%- if devise_mapping.registerable? %>
      <%# <%= link_to 'Cadastrar', new_registration_path(resource_name), class: 'right'  %>
    <% end -%>

    <div class="row" padding-top>
      <div class="card">
        <div class="card-content">
          <div class="input-field"> 
            <%= f.email_field :email, placeholder: 'E-mail', autofocus: true, class: 'validate' %>
            <span class="helper-text" data-error="Verifique seu e-mail" data-success="Ã“timo!"></span>
          </div>

          <%#- if devise_mapping.recoverable? %>
            <%# <%= link_to "Esqueci minha senha", new_password_path(resource_name) %>
          <%# end -%>
            
          <div class="input-field">
            <%= f.password_field :password, placeholder: 'Senha', class: 'validate' %>
          </div>

          <%# if devise_mapping.rememberable? -%>
            <%# = f.check_box :remember_me%> <%#Lembrar de mim %>
          <%# end -%>
          <div class="actions center">
            <%= f.submit 'Entrar', class: 'waves-effect waves-light btn blue-grey darken-1' %>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>