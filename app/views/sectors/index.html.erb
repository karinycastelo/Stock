<%# <p id="notice"><%= notice %>

<div class="card">
  <div class="card-content">
  <table class="responsive-table highlight">
      <thead>
        <tr>
          <th>Nome</th>
          <th>EndereÃ§o</th>
          <th>Secretaria</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @sectors.each do |sector| %>
        <%# <tr data-link="sectors/<%=sector.id>"> %>
        <tr>
          <td><%= sector.name %></td>
          <td><%= sector.address %></td>
          <td><%= sector.secretary.name %></td>
          <td><%= link_to 'Editar', edit_sector_path(sector), class: "mdl-button mdl-js-button mdl-button--primary" %></td>
          <td><%= link_to 'Excluir', sector, method: :delete, data: { confirm: 'Tem certeza?' }, class: "mdl-button mdl-js-button mdl-button--primary" %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

<div class="fixed-action-btn tooltipped" style="margin-bottom: 36px;">
  <%= link_to new_sector_path, class: "btn-floating btn-large waves-effect waves-light blue-grey darken-1", method: :get do %>
    <i id="add"class="material-icons">add</i>
  <% end %>
</div>