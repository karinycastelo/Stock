<%# <p id="notice"><%= notice %>
<div class="mdl-card ssc_card">
  <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
  <thead>
    <tr>
      <th class= "mdl-data-table__cell--non-numeric">Nome</th>
      <th class= "mdl-data-table__cell--non-numeric">EndereÃ§o</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @secretaries.each do |secretary| %>
      <tr>
        <td><%= secretary.name %></td>
        <td><%= secretary.address %></td>
        <td><%= link_to 'Mostrar', secretary %></td>
        <td><%= link_to 'Editar', edit_secretary_path(secretary) %></td>
        <td><%= link_to 'Excluir', secretary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>


<div class="mdl-typography--text-right fixed-action-btn tooltipped">
  <%= link_to new_secretary_path, class: "mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-button--colored", method: :get do %>
    <i id="add"class="material-icons">add</i>
    <div class="mdl-tooltip mdl-tooltip--large" for="add">
      Nova Secretaria
    </div>
  <% end %>
</div>