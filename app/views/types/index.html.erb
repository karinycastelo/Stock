<%# <p id="notice"><%= notice %>

<div class="card">
  <div class="card-content">
  <table class="responsive-table highlight">
      <thead>
        <tr>
          <th>Descrição</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @types.each do |type| %>
          <tr>
            <%# <tr data-link="enterprises/<%=enterprise.id>"> %>
            <td><%= type.description %></td>
            <td><%= link_to 'Editar', edit_type_path(type), class: "mdl-button mdl-js-button mdl-button--primary" %></td>
            <td><%= link_to 'Excluir', type, method: :delete, data: { confirm: 'Tem certeza' }, class: "mdl-button mdl-js-button mdl-button--primary" %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>

<div class="fixed-action-btn tooltipped" style="margin-bottom: 36px;">
  <%= link_to new_type_path, class: "btn-floating btn-large waves-effect waves-light blue-grey darken-1", method: :get do %>
    <i id="add"class="material-icons">add</i>
  <% end %>
</div>